<div class="container-fluid">
  <div class="row">
    <div class="col-md-5 col-sm-5 col-xs-5 col-lg-5">
      <div class="mainbar">
        <a href="javascript:" id="memoryId" class="zoomAll" data-id="{{memoryId}}">{{ memName }}</a>
      </div>
      <div id="name" class="place-name"></div>
      <div class="img-container">
        <a href="javascript:"><img id="arrowPrev" class="arrow" alt="Next point" src="/images/picturepanel-left.png" ></a>
        <img id="mainImage" class="imgMainStyle" src="/images/Made-for-Memories.jpg" alt="Made-for-Memories.jpg"/>
        <a href="javascript:"><img id="arrowNext" class="arrow" alt="Next point" src="/images/picturepanel-right.png" ></a>
      </div>
      <div id="desc" class="place-desc"></div>
      {{!-- <div class="thumbnail"></div>     --}}
    </div>
    <div class="col-md-7 col-sm-7 col-xs-7 col-lg-7 lealet-map">
      <section id="map"></section>
    </div>
  </div>
</div>

